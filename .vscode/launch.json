{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch CodacyVisualBasic.Analyzer",
			"type": "mono",
			"request": "launch",
			"program": "${workspaceRoot}/src/Analyzer/bin/Debug/net461/Analyzer.exe",
			"env": {
				"DEBUG": "true",
				"TIMEOUT": "15 minutes"
			},
			"cwd": "${workspaceRoot}/.tests/",
			"preLaunchTask": "CodacyVisualBasic.Analyzer"
		},
		{
			"name": "Launch CodacyVisualBasic.DocsGenerator",
			"type": "mono",
			"request": "launch",
			"program": "${workspaceRoot}/src/DocsGenerator/bin/Debug/net461/DocsGenerator.exe",
			"cwd": "${workspaceRoot}",
			"preLaunchTask": "CodacyVisualBasic.DocsGenerator"
		},
		{
			"name": "Attach",
			"type": "mono",
			"request": "attach",
			"address": "localhost",
			"port": 55555
		}
	]
}
